{
  "version": 2,
  "builds": [
    {
      "src": "./backend/dir/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "./frontend/dist",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/dir/app.js"
    },
    {
      "source": "/(.*)",
      "destination": "./frontend/dist/index.html"
    }
  ]
}
